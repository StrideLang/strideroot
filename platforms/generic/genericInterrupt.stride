
# Generic peripherals
genericOption GENERIC_INTERRUPT_MODE {
    name: "Mode"
    values: ["Rising Edge", "Falling Edge", "Rising/Falling Edge"]
    meta: ""
}

genericOption GENERIC_INTERRUPT_MASK {
    name: "Interrupt Masked"
    values: ["Masked (Disabled)", "Unmasked (Enabled)"]
    meta: ""
}

genericPeripheral GENERIC_INTERRUPT {
    name: "Interrupt"
    parameters: [GENERIC_INTERRUPT_MODE, GENERIC_INTERRUPT_MASK]
    meta: "Interrupt"
}